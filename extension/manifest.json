{
    "manifest_version": 2,
    "name": "__MSG_extName__",
    "short_name": "__MSG_extShortName__",
    "version": "2.4.2",
    "description": "__MSG_extDesc__",
    "applications": {
        "gecko": {
          "id": "tomas.yangjianchuan@gmail.com",
          "strict_min_version": "58.0"
        }
    },
    "background": {
        "page": "background.html"
    },
    "default_locale": "zh_CN",
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_title": "Word Card",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "css": ["content.css"],
        "all_frames": true,
        "js": ["manifest.js", "vendor.js", "content.js"]
    }],
    "icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    "web_accessible_resources": [
        "img/*",
        "translate.html"
    ],
    "omnibox": { "keyword" : "wd" },
    "options_ui": {
        "page": "options.html",
        "browser_style": true
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://localhost/* https://ssl.google-analytics.com; object-src 'self'",
    "permissions": [
        "tabs",
        "cookies",
        "clipboardRead",
        "clipboardWrite",
        "contextMenus",
        "storage",
        "notifications",
        "<all_urls>"
    ],
    "commands": {
     
    }
}
